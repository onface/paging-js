        "karma": "^2.0.0",
        "karma-babel-preprocessor": "^7.0.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-jasmine": "^1.1.1",
        "karma-sauce-launcher": "^1.2.0",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-webpack": "^2.0.9",
        "less": "^2.7.3",
        "less-loader": "^2.2.3",
        "less-plugin-autoprefix": "^1.5.1",
        "less-plugin-functions": "^1.0.0",
        "livereload": "^0.6.3",
        "markrun": "^0.23.0",
        "mustache": "^2.3.0",
        "open": "^0.0.5",
        "prop-types": "0.2.0",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-hot-loader": "^4.0.0-beta.6",
        "style-loader": "^0.13.1",
